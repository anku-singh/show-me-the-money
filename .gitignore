# Ignore node_modules in both frontend and backend
/frontend/node_modules/
/backend/node_modules/

# Ignore build outputs for frontend
/frontend/build/
/frontend/dist/

# Ignore build outputs for backend
/backend/build/
/backend/dist/

# Ignore environment files in both frontend and backend
/frontend/.env
/backend/.env

# Ignore log files
/frontend/logs
/frontend/*.log
/frontend/npm-debug.log*
/backend/logs
/backend/*.log
/backend/npm-debug.log*

# Ignore IDE-specific files (optional)
.vscode/
.idea/

# Ignore macOS system files
.DS_Store

# Ignore any temporary files
*.tmp
*.swp
